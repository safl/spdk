framework_start_init
nvmf_create_transport -t TCP -u 10240 -m 8 -c 8192 -f -m 128
bdev_null_create Null1 100 520 -m 8 -t 1
bdev_dif_create -b Null1 -p Dif1
nvmf_create_subsystem nqn.2016-06.io.spdk:cnode1 -a -s SPDK00000000000001 -d SPDK_Controller1
nvmf_subsystem_add_ns nqn.2016-06.io.spdk:cnode1 Dif1

nvmf_subsystem_add_listener discovery -t tcp -a 127.0.0.1 -s 4410

nvmf_subsystem_add_listener nqn.2016-06.io.spdk:cnode1 -t tcp -a 127.0.0.1 -s 4420